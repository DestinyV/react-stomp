{
  "name": "react-stomp",
  "version": "3.3.0",
  "description": "React websocket component with STOMP over SockJS",
  "main": "./dist/client.js",
  "scripts": {
    "prepublish": "npm run test && npm run build",
    "lint": "eslint src/* test/*",
    "lint-fix": "eslint src/* test/* --fix",
    "test": "npm run lint && mocha --require babel-core/register --require test/bootstrap.js \"test/*.test.js\" --exit",
    "build": "babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lahsivjar/react-stomp.git"
  },
  "keywords": [
    "react",
    "websocket",
    "sockjs",
    "stomp"
  ],
  "author": "lahsivjar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lahsivjar/react-stomp/issues"
  },
  "homepage": "https://github.com/lahsivjar/react-stomp#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.5.1",
    "jsdom": "^11.5.1",
    "lodash": "^4.17.5",
    "mocha": "^4.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sinon": "^4.1.3"
  },
  "dependencies": {
    "net": "^1.0.2",
    "sockjs-client": "^1.1.4",
    "stompjs": "^2.3.3"
  }
}
